.XTerm*metaSendsEscape: true
xterm*eightBitInput: false
urxvt*scrollBar: false

 ! Zenburn colortheme for rxvt
urxvt*background:   #111111
urxvt*foreground:   #dcdccc
urxvt*cursorColor:  #cdc092
 !black
urxvt*color0:       #111111
 !red
urxvt*color1:       #705050
 !green
urxvt*color2:       #60b48a
 !yellow
urxvt*color3:       #dfaf8f
 !blue
urxvt*color4:       #506070
 !purple
urxvt*color5:       #dc8cc3
 !cyan
urxvt*color6:       #8cd0d3
 !white
urxvt*color7:       #dcdccc
 !bright-black
urxvt*color8:       #709080
 !bright-red
urxvt*color9:       #dca3a3
 !bright-green
urxvt*color10:      #c3bf9f
 !bright-yellow
urxvt*color11:      #f0dfaf
 !bright-blue
urxvt*color12:      #94bff3
 !bright-purple
urxvt*color13:      #ec93d3
 !bright-cyan
urxvt*color14:      #93e0e3
 !bright-white
urxvt*color15:      #ffffff

