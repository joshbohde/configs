(*-----------------------------------------------------------------------*)
Valid_ident		_1		_a		AaAaAa_Aa	testing1_	POLLY9
Invalid_ident	1_		testing1,2,3
(*-----------------------------------------------------------------------*)
Valid_int		0		1		+1			-1				-2147483647
				000000002147483647			+001			-0
Invalid_int		++12	--12	2-1			0002147483648	2147483648
(*-----------------------------------------------------------------------*)
Valid_char		'a'		'\n'	'\+'		'\''			'\	'	' '
Invalid_char	'		''		'''			'd				'dog'
(*-----------------------------------------------------------------------*)
Valid_tokens	()*+,-...::=;<<=<>=>>=[]'
				and array begin boolean char div do else end false
				if integer not of or procedure program read then
				true var while write
Invalid_tokens	andarraybeginbooleanchardivdoelseendfalse
				ifintegernotoforprocedureprogramreadthen
				truevarwhilewrite
(*-----------------------------------------------------------------------*)
Unknown			&#$
(*-----------------------------------------------------------------------*)
Combinations	(2+2=4)	2++2=5	Mary_4+Bob_6=Tim_10
				c=sqrt(a*a div b*b);
				c=sqrt(a*adivb*b);
(*-----------------------------------------------------------------------*)
